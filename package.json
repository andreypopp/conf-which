{
    "name": "conf-which",
    "version": "1.0.0",
    "dependencies": {
        "opam-installer-bin": "andreypopp/opam-installer-bin"
    },
    "esy": {
        "build": [
          "which which",
          "(opam-installer --prefix=$cur__install || true)"
        ],
        "exportedEnv": {
            "conf_which_version": {
                "scope": "global",
                "val": "1.0.0"
            },
            "conf_which_enable": {
                "scope": "global",
                "val": "enable"
            },
            "conf_which_installed": {
                "scope": "global",
                "val": "true"
            }
        }
    },
    "peerDependencies": {
        "ocaml": "andreypopp/ocaml#esy"
    }
}
